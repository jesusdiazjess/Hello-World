#include<stdio.h>

int main()
{

    char name[36];
    char course[36];
    char yearlevel[36];
    char subject[36];
    char subjectinstructor[36];
    
    printf("Enter name: ");
    fgets(name, sizeof(name), stdin);
    printf("Name is ");
    puts(name);
    
    printf("Enter course: ");
    fgets(course, sizeof(course), stdin);
    printf("Course is ");
    puts(course);  
    
    printf("Enter year level: ");
    fgets(yearlevel, sizeof(yearlevel), stdin);
    printf("Year level is ");
    puts(yearlevel);   
    
    printf("Enter subject: ");
    fgets(subject, sizeof(subject), stdin);
    printf("Subject is ");
    puts(subject);      
    
    printf("Enter subject instructor: ");
    fgets(subjectinstructor, sizeof(subjectinstructor), stdin);
    printf("Subject instructor is ");
    puts(subjectinstructor);
    
    return 0;
}
